{
	"compilerOptions": {
    "target": "ES2021",
    "module": "nodenext",
    "moduleResolution": "NodeNext",
    "lib": ["ES2021", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "prisma.config.ts"]
}